  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Launch Server",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/outreach_backend",
      "envFile": "${workspaceFolder}/outreach_backend/.env",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "console": "integratedTerminal"
    }
  ]
}